      mouth: null,
      eyes: null,
      beard: null,
      hair: null,
      clothes: null,
      accessory: null,
    }

    function redraw() {
      ctx.clearRect(0, 0, 500, 500);
      for (const entry of Object.entries(currentSelection)) {
        var property = document.getElementById(entry[0] + "-" + entry[1]);
        var image = new Image();
        image.src = '/images/qeycard/' +entry[0] + '/' + entry[1] + '.png';
        ctx.drawImage(image, 0, 0, 500, 500 * image.height / image.width);
      }
    }

    function randomize() {
      options = [];
      options.push(document.getElementsByClassName('background-option'));
      options.push(document.getElementsByClassName('skin-option'));
      options.push(document.getElementsByClassName('mouth-option'));
      options.push(document.getElementsByClassName('eyes-option'));
      options.push(document.getElementsByClassName('beard-option'));
      options.push(document.getElementsByClassName('hair-option'));
      options.push(document.getElementsByClassName('clothes-option'));
      options.push(document.getElementsByClassName('accessory-option'));
      for (const i in options) {
        options[i][Math.floor(Math.random() * options[i].length)].click();

      }
      redraw();
    }

    function loadAssets() {
      for (const entry of Object.entries(assets)) {
        var category = entry[0];
        var options = entry[1]
        for (const value of Object.values(options)) {
          var label = document.createElement('label');
          var option = document.createElement('input');
          var selected = null;
          option.setAttribute("type", "radio")
          option.id = category + '-' + value.name;
          label.style.backgroundImage = 'url(/images/qeycard/' + category + '/' + value.name + '.png)';
          label.setAttribute("for", category + '-' + value.name);
          label.setAttribute("alt", value.name)
          label.setAttribute("title", value.name)
          option.id = category + '-' + value.name;
          option.value = value.name;
          option.name = category;
          option.className = category + '-option';
          label.appendChild(option);
          option.addEventListener('change', function () {
            currentSelection[this.name] = this.value;
            currentName[this.name] = this.flavour;
            redraw();
          });
          document.getElementById(category).appendChild(label);
        }

      }
    }

    function save() {

      var link = document.createElement('a');
      link.download = 'qeycard.png';
      link.href = c.toDataURL('image/png')
      link.click();
    }

    window.onload = function () {
      loadAssets();
      c = document.getElementById("canvas");
      ctx = c.getContext("2d");
      const randomizeButton = document.getElementById("randomize");
      randomizeButton.addEventListener('click', function () {
        randomize();
      });
      redraw();
    }
