    var currentSelection = {
      background: null,
      skin: null,
      base: "base",
      mouth: null,
      eyes: null,
      beard: null,
      hair: null,
      clothes: null,
      accessory: null,
    };

    var currentName = {
      background: null,
      skin: null,
